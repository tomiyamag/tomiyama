{
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "node": true,
    "commonjs": true,
    "es6": true,
    "jquery": true
  },
  "rules": {
    "no-undef": "warn",
    "no-console": "warn",
    "no-unused-vars": "warn"
  }
}
